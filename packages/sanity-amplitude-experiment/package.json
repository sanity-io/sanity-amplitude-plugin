{
  "name": "@tinloof/sanity-amplitude-experiment",
  "type": "module",
  "version": "0.0.3",
  "description": "A plugin to easily connect your Amplitude experiments with your Sanity documents.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tinloof/sanity-amplitude-experiment.git",
    "directory": "packages/sanity-amplitude-experiment"
  },
  "keywords": [
    "sanity",
    "sanity-plugin",
    "amplitude",
    "tinloof",
    "ab testing"
  ],
  "license": "MIT",
  "author": "Tinloof",
  "sideEffects": false,
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "dev": "run-s watch",
    "build": "pkg build --strict --check --clean",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "watch": "pkg watch --strict",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@sanity/icons": "^2.11.8",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/studio-secrets": "^2.0.2",
    "@sanity/ui": "^2.1.4",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^6.7.1",
    "@sanity/plugin-kit": "^4.0.5",
    "@types/react": "^18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanity": "^3.38.0",
    "styled-components": "^6.1.8",
    "typescript": "^5.4.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "sanity",
      "sanity/typescript",
      "sanity/react",
      "plugin:react-hooks/recommended",
      "plugin:prettier/recommended",
      "plugin:react/jsx-runtime"
    ],
    "ignores": [
      "*.js",
      ".eslintrc.js",
      "commitlint.config.js",
      "dist",
      "lint-staged.config.js",
      "package.config.ts"
    ]
  },
  "peerDependencies": {
    "react": "^18.3 || ^19",
    "sanity": "^3 || ^4.0.0-0"
  },
  "engines": {
    "node": ">=18"
  }
}
