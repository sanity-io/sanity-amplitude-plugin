    "@sanity/ui": "^2.0.14",
    "@sanity/vision": "^3.36.4",
    "@tinloof/sanity-amplitude-experiment": "workspace:*",
    "@tinloof/sanity-web": "^0.1.1",
    "class-variance-authority": "^0.7.0",
    "next": "14.1.4",
